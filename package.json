{
  "name": "ember-cli-postcss",
  "version": "7.0.2",
  "description": "Use postcss to process your css with a large selection of JavaScript plug-ins.",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "deploy": "ember deploy production",
    "test": "npm run lint && ember try:each",
    "lint": "eslint ."
  },
  "engines": {
    "node": ">= 10"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "files": ["blueprints", "config/environment.js", "index.js"],
  "repository": {
    "type": "git",
    "url": "https://github.com/jeffjewiss/ember-cli-postcss"
  },
  "bugs": {
    "url": "https://github.com/jeffjewiss/ember-cli-postcss/issues"
  },
  "homepage": "https://github.com/jeffjewiss/ember-cli-postcss",
  "author": "Jeff Jewiss <jeffjewiss@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "broccoli-merge-trees": "4.2.0",
    "broccoli-postcss": "6.0.1",
    "broccoli-postcss-single": "5.0.1",
    "ember-cli-babel": "^7.26.11",
    "merge": "2.1.1"
  },
  "keywords": [
    "ember-addon",
    "css",
    "prefix",
    "preprocess",
    "postprocess",
    "postcss"
  ],
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://jeffjewiss.github.io/ember-cli-postcss",
    "before": ["ember-cli-sri", "broccoli-asset-rev"]
  },
  "standard": {
    "ignore": ["testem.js"]
  },
  "devDependencies": {
    "babel-eslint": "^10.0.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "^3.10.1",
    "ember-cli-app-version": "^5.0.0",
    "ember-cli-dependency-checker": "^3.0.0",
    "ember-cli-deploy": "^1.0.1",
    "ember-cli-deploy-build": "^2.0.0",
    "ember-cli-deploy-git": "^1.1.1",
    "ember-cli-eslint": "^5.0.0",
    "ember-cli-fastboot": "^2.0.0",
    "ember-cli-htmlbars": "^6.0.0",
    "ember-cli-qunit": "^4.3.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^3.0.0",
    "ember-code-prettify": "^0.0.1",
    "ember-disable-prototype-extensions": "^1.1.0",
    "ember-load-initializers": "^2.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-resolver": "8.0.2",
    "ember-router-scroll": "^3.2.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-try": "^1.2.1",
    "ember-typeface": "^1.0.0",
    "eslint": "^7.12.1",
    "eslint-config-standard": "^14.0.1",
    "eslint-plugin-ember": "^9.3.0",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^4.0.7",
    "loader.js": "^4.1.0",
    "minimatch": "^3.0.0",
    "postcss-color-function": "^4.1.0",
    "postcss-color-gray": "^5.0.0",
    "postcss-custom-properties": "^11.0.0",
    "postcss-import": "^14.0.0",
    "postcss-nesting": "^8.0.1",
    "prember": "^1.0.0",
    "tachyons": "^4.7.4",
    "typeface-lato": "^1.1.13",
    "typeface-oxygen": "^1.1.13"
  },
  "volta": {
    "node": "16.13.1"
  }
}
