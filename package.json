{
  "name": "ember-cli-postcss",
  "version": "8.2.0",
  "description": "Use postcss to process your css with a large selection of JavaScript plug-ins.",
  "keywords": [
    "ember-addon",
    "css",
    "prefix",
    "preprocess",
    "postprocess",
    "postcss"
  ],
  "homepage": "https://github.com/jeffjewiss/ember-cli-postcss",
  "bugs": {
    "url": "https://github.com/jeffjewiss/ember-cli-postcss/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jeffjewiss/ember-cli-postcss"
  },
  "license": "MIT",
  "author": "Jeff Jewiss <jeffjewiss@gmail.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "files": [
    "blueprints",
    "config/environment.js",
    "index.js"
  ],
  "scripts": {
    "build": "ember build",
    "deploy": "ember deploy production",
    "lint": "eslint .",
    "start": "ember server",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "dependencies": {
    "broccoli-merge-trees": "^4.2.0",
    "broccoli-postcss": "^6.0.1",
    "broccoli-postcss-single": "^5.0.1",
    "ember-cli-babel": "^7.26.11",
    "merge": "^2.1.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.6.0",
    "@glimmer/component": "^1.0.0",
    "@glimmer/tracking": "^1.0.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^2.2.4",
    "ember-cli": "^4.1.0",
    "ember-cli-app-version": "^5.0.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-deploy": "^1.0.1",
    "ember-cli-deploy-build": "^2.0.0",
    "ember-cli-deploy-git": "^1.1.1",
    "ember-cli-eslint": "^5.0.0",
    "ember-cli-htmlbars": "^6.0.0",
