{
  "name": "ember-cli-postcss",
  "version": "3.2.0",
  "description": "Use postcss to process your css with a large selection of JavaScript plug-ins.",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "npm run lint && ember try:each",
    "lint": "eslint config *.js"
  },
  "files": [
    "config/environment.js",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jeffjewiss/ember-cli-postcss"
  },
  "bugs": {
    "url": "https://github.com/jeffjewiss/ember-cli-postcss/issues"
  },
  "homepage": "https://github.com/jeffjewiss/ember-cli-postcss",
  "author": "Jeff Jewiss <jeffjewiss@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "bower": "1.8.0",
    "broccoli-merge-trees": "2.0.0",
    "broccoli-postcss": "3.3.0",
    "broccoli-postcss-single": "1.2.1",
    "ember-cli-dependency-checker": "1.3.0",
    "merge": "1.2.0"
  },
  "keywords": [
    "ember-addon",
    "css",
    "prefix",
    "preprocess",
    "postprocess",
    "postcss"
  ],
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "before": [
      "ember-cli-sri",
      "broccoli-asset-rev"
    ]
  },
  "standard": {
    "ignore": [
      "testem.js"
    ]
  },
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.2",
    "ember-cli": "^2.12.1",
    "ember-cli-app-version": "^3.0.0",
    "ember-cli-babel": "^5.2.1",
    "ember-cli-eslint": "^3.0.2",
    "ember-cli-htmlbars": "^1.0.1",
    "ember-cli-htmlbars-inline-precompile": "^0.3.1",
    "ember-cli-inject-live-reload": "^1.4.0",
    "ember-cli-qunit": "^3.0.0",
    "ember-cli-release": "^0.2.9",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-test-loader": "^2.0.0",
    "ember-cli-uglify": "^1.2.0",
    "ember-disable-prototype-extensions": "^1.1.0",
    "ember-disable-proxy-controllers": "^1.0.1",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.0.0",
    "ember-resolver": "^4.0.0",
    "eslint-config-standard": "^7.0.0",
    "eslint-plugin-ember-suave": "^1.0.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "loader.js": "^4.1.0",
    "minimatch": "^3.0.0",
    "postcss-color-gray": "^3.0.1",
    "postcss-color-rebeccapurple": "^2.0.1"
  }
}
