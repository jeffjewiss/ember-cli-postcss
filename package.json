{
  "name": "ember-cli-postcss",
  "version": "4.0.0",
  "description": "Use postcss to process your css with a large selection of JavaScript plug-ins.",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "deploy": "ember deploy production",
    "test": "npm run lint && ember try:each",
    "test:appveyor": "ember test",
    "lint": "eslint ./*.js app addon config server test-support tests",
    "lint:fix": "./*.js eslint app addon config server test-support tests --fix"
  },
  "engines": {
    "node": ">= 6.9"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "files": [
    "blueprints",
    "config/environment.js",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jeffjewiss/ember-cli-postcss"
  },
  "bugs": {
    "url": "https://github.com/jeffjewiss/ember-cli-postcss/issues"
  },
  "homepage": "https://github.com/jeffjewiss/ember-cli-postcss",
  "author": "Jeff Jewiss <jeffjewiss@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "broccoli-file-creator": "^2.1.1",
    "broccoli-merge-trees": "^3.0.0",
    "broccoli-postcss": "^3.5.3",
    "broccoli-postcss-single": "^2.0.0",
    "ember-cli-babel": "^6.12.0",
    "merge": "^1.2.0"
  },
  "keywords": [
    "ember-addon",
    "css",
    "prefix",
    "preprocess",
    "postprocess",
    "postcss"
  ],
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://jeffjewiss.github.io/ember-cli-postcss",
    "before": [
      "ember-cli-sri",
      "broccoli-asset-rev"
    ]
  },
  "standard": {
    "ignore": [
      "testem.js"
    ]
  },
  "devDependencies": {
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "^3.0.0",
    "ember-cli-app-version": "^3.0.0",
    "ember-cli-dependency-checker": "^3.0.0",
    "ember-cli-deploy": "^1.0.1",
    "ember-cli-deploy-build": "^1.1.0",
    "ember-cli-deploy-git": "^1.1.1",
    "ember-cli-eslint": "^4.0.0",
    "ember-cli-fastboot": "^1.1.3",
    "ember-cli-htmlbars": "^3.0.0",
    "ember-cli-htmlbars-inline-precompile": "^1.0.1",
    "ember-cli-inject-live-reload": "^1.4.0",
    "ember-cli-qunit": "^4.3.0",
    "ember-cli-release": "^0.2.9",
    "ember-cli-shims": "^1.1.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-code-prettify": "^0.0.1",
    "ember-disable-prototype-extensions": "^1.1.0",
    "ember-disable-proxy-controllers": "^1.0.1",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-resolver": "^5.0.0",
    "ember-router-scroll": "^0.7.0",
    "ember-source": "^3.0.0",
    "ember-source-channel-url": "^1.0.1",
    "ember-try": "^0.2.23",
    "ember-typeface": "^0.3.0",
    "eslint": "^4.17.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-ember": "^5.0.3",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.0",
    "husky": "1.0.0-rc.8",
    "loader.js": "^4.1.0",
    "minimatch": "^3.0.0",
    "postcss-color-function": "4.0.1",
    "postcss-color-gray": "^4.0.0",
    "postcss-custom-properties": "^7.0.0",
    "postcss-import": "^12.0.0",
    "postcss-nesting": "^6.0.0",
    "prember": "^0.3.0",
    "tachyons": "^4.7.4",
    "typeface-lato": "0.0.54",
    "typeface-oxygen": "0.0.54"
  }
}
